    "@fastify/helmet": "^10.1.0",
    "@fastify/static": "^6.9.0",
    "@theconvospace/sdk": "^0.6.16",
    "@upstash/ratelimit": "^0.4.1",
    "@upstash/redis": "^1.20.2",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "eth-phishing-detect": "^1.2.0",
